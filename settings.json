{
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "explorer.compactFolders": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "files.autoSave": "afterDelay",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "'Operator Mono'",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "300",
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Solarized Dark",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "workbench.preferredDarkColorTheme": "Solarized Dark",
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "vscode-icons",
  "editor.fontFamily": "'Operator Mono'",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": false,
  "editor.rulers": [120],
  "editor.tokenColorCustomizations": {
    "numbers": "#df2a63",
    "strings": "#15b5ac",
    "textMateRules": [
      {
        "name": "JSON & SCSS property keys, keywords",
        "scope": ["support.type", "support.class", "keyword"],
        "settings": {
          "foreground": "#2fa84f"
        }
      },
      {
        "name": "class names",
        "scope": [
          "entity.name.class",
          "entity.name.type",
          "entity.name.namespace",
          "entity.name.scope-resolution"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "inherited class",
        "scope": "entity.other.inherited-class",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "storage",
        "scope": "storage",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "attrs/props/listeners",
        "scope": "entity.other.attribute-name",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "units",
        "scope": "keyword.other.unit",
        "settings": {
          "foreground": "#15b5ac"
        }
      },
      {
        "name": "contants (e.g. true, false)",
        "scope": "constant.language",
        "settings": {
          "foreground": "#bfb541"
        }
      },
      {
        "name": "regular expressions",
        "scope": "string.regexp",
        "settings": {
          "foreground": "#e60a0a"
        }
      }
    ]
  },
  "workbench.colorCustomizations": {
    "diffEditor.insertedTextBackground": "#2fa84f50",
    "diffEditor.removedTextBackground": "#e60a0a50",
    "editor.findMatchBackground": "#84085390",
    "editor.findMatchHighlightBackground": "#84085340",
    "editor.selectionBackground": "#22c2ad3d",
    "editor.selectionHighlightBackground": "#84085340",
    "editorGutter.addedBackground": "#2fa84f",
    "editorGutter.modifiedBackground": "#15b5ac",
    "editorGutter.deletedBackground": "#e60a0a",
    "editorOverviewRuler.addedForeground": "#2fa84f80",
    "editorOverviewRuler.modifiedForeground": "#15b5ac",
    "editorOverviewRuler.deletedForeground": "#e60a0a80",
    "editorOverviewRuler.warningForeground": "#fcc75180",
    "editorOverviewRuler.errorForeground": "#e60a0a80",
    "gitDecoration.addedResourceForeground": "#2fa84f",
    "gitDecoration.untrackedResourceForeground": "#2fa84f",
    "gitDecoration.modifiedResourceForeground": "#15b5ac",
    "gitDecoration.deletedResourceForeground": "#e60a0a",
    "list.errorForeground": "#e60a0a99",
    "terminal.ansiBlack": "#303030",
    "terminal.ansiBrightBlack": "#999999",
    "terminal.ansiRed": "#e60a0a80",
    "terminal.ansiBrightRed": "#e60a0a",
    "terminal.ansiGreen": "#2fa84f",
    "terminal.ansiBrightGreen": "#2fa84f",
    "terminal.ansiYellow": "#fcc751",
    "terminal.ansiBrightYellow": "#fcc751",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiBrightBlue": "#268bd2",
    "terminal.ansiMagenta": "#df2a63",
    "terminal.ansiBrightMagenta": "#f2828f",
    "terminal.ansiCyan": "#15b5ac",
    "terminal.ansiBrightCyan": "#20e1d1"
  },
